# Oura API Credentials
# Copy this file to .env and fill in your actual values
# NEVER commit your .env file to GitHub!

# Get these from: https://developer.ouraring.com/applications/your_app_id
CLIENT_ID=your_client_id_here
CLIENT_SECRET=your_client_secret_here

# This will be generated after you complete the OAuth flow
ACCESS_TOKEN=your_access_token_here
REFRESH_TOKEN=your_refresh_token_here

# Redirect URI (must match what you set in Oura Developer Portal)
REDIRECT_URI=http://localhost:8080
